        <amfBCMPSU node="rbm-fe-s1-h1" commitSize="20" consumerGroup="CG1" topicName="bucket-expiry-topic" />
        <amfBCMPSU node="rbm-fe-s1-h1" commitSize="20" consumerGroup="CG2" topicName="price-plan-change-topic" />
