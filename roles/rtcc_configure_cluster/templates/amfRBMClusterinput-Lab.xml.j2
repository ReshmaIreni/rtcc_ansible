<?xml version="1.0" encoding="US-ASCII" standalone="no"?>
<amfconfig xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="./amfRBMClusterinput.xsd">
    <alias clustername="{{ CLUSTER_NAME }}" clustertype="RBM Cluster" clustertypeid="1" infinysroot="{{ HA_ROOT }}/INFINYS" platform="LINUX">
        <node immpm="no" name="rbm-fe-s1-h1"/>
        <node immpm="no" name="rbm-fe-s2-h1"/>
    </alias>

    <app cacheonly="yes" name="Online Charging App for RBM Cluster" sdfRASL="no" sdfStandin="no">
        <!--
        ************************************
        *    TELENET REQUIRED PROCESSES    *
        ************************************
        -->

    <amfCacheSG>
        <amfCacheSU cache="TLNT02A" node="rbm-fe-s1-h1"/>
        <amfCacheSU cache="TLNT02B" node="rbm-fe-s1-h1"/>
    </amfCacheSG>
    <amfCacheSG>
        <amfCacheSU cache="TLNT03A" node="rbm-fe-s1-h2"/>
        <amfCacheSU cache="TLNT03B" node="rbm-fe-s1-h2"/>
    </amfCacheSG>
    <amfCacheSG>
        <amfCacheSU cache="TLNT04A" node="rbm-fe-s2-h1"/>
        <amfCacheSU cache="TLNT04B" node="rbm-fe-s2-h1"/>
    </amfCacheSG>
    <amfCacheSG>
        <amfCacheSU cache="TLNT05A" node="rbm-fe-s2-h2"/>
        <amfCacheSU cache="TLNT05B" node="rbm-fe-s2-h2"/>
    </amfCacheSG>
   

    <amfRaterSG amfRaterCompCount="2">
            <amfRaterSU bufferSize="20" commitSize="10" commport="14301" servicename="RBM_RATE_SERVICE" node="rbm-fe-s1-h1" ratingPoolId="1" />
            <amfRaterSU bufferSize="20" commitSize="10" commport="14301" servicename="RBM_RATE_SERVICE" node="rbm-fe-s1-h2" ratingPoolId="1" />
            <amfRaterSU bufferSize="20" commitSize="10" commport="14301" servicename="RBM_RATE_SERVICE" node="rbm-fe-s2-h1" ratingPoolId="1" />
            <amfRaterSU bufferSize="20" commitSize="10" commport="14301" servicename="RBM_RATE_SERVICE" node="rbm-fe-s2-h2" ratingPoolId="1" />
    </amfRaterSG>

    <amfOELSSG amfOELSCompCount="1" targetCache="TLNT02A,TLNT02B,TLNT03A,TLNT03B,TLNT04A,TLNT04B,TLNT05A,TLNT05B">
            <amfOELSSU bufferSize="21" commitSize="20" elfBufferSize="1" elfCommitSize="1" node="rbm-fe-s1-h1"/>
            <amfOELSSU bufferSize="21" commitSize="20" elfBufferSize="1" elfCommitSize="1" node="rbm-fe-s1-h1"/>
            <amfOELSSU bufferSize="21" commitSize="20" elfBufferSize="1" elfCommitSize="1" node="rbm-fe-s1-h2"/>
