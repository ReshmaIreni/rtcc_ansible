    <amfUDRUSG amfUDRUCompCount="1" targetDB="TLNT02A,TLNT02B,TLNT03A,TLNT03B,TLNT04A,TLNT04B,TLNT05A,TLNT05B">
        <amfUDRUSU d="5000" mode="priming" node="rbm-fe-s1-h1"/>
