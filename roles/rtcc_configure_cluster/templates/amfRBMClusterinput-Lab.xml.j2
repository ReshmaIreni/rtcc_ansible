    </amfBCMPSG>

    <amfOELCSG amfOELCCompCount="1" targetDG="1,2,3,4">
            <amfOELCSU cache="TLNT01" bufferSize="21" commitSize="20" elfBufferSize="1" elfCommitSize="1" node="rbm-fe-s1-h1"/>
            <amfOELCSU cache="TLNT01" bufferSize="21" commitSize="20" elfBufferSize="1" elfCommitSize="1" node="rbm-fe-s1-h2"/>
