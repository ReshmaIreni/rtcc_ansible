            <amfRDLSU node="rbm-fe-s2-h1"/>
            <amfRDLSU node="rbm-fe-s2-h2"/>
            <amfRDLSU node="rbm-fe-s2-h2"/>
    </amfRDLSG>

    <amfDUMSG amfDUMExtCompCount="1" amfDUMProdCompCount="1" targetCache="TLNT02A,TLNT02B,TLNT03A,TLNT03B,TLNT04A,TLNT04B,TLNT05A,TLNT05B">
